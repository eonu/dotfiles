#!/usr/bin/env bash

# rbenv
eval "$(rbenv init - zsh)"

# brew
eval "$(brew shellenv)"

# nvm
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh" # Loads NVM
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # Loads NVM bash completion

# display dircolors (ls, tree, etc.)
eval "$(gdircolors -b ~/.dir_colors)"

# fzf
[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh
